{
  "name": "stm-sendmail",
  "version": "1.1.1",
  "author": "stm",
  "summary": "Install and manage the Sendmail MTA.",
  "license": "BSD-2-Clause",
  "source": "https://github.com/smoeding/puppet-sendmail",
  "project_page": "https://github.com/smoeding/puppet-sendmail",
  "issues_url": "https://github.com/smoeding/puppet-sendmail/issues",
  "tags": [ "mail", "email", "mta", "sendmail", "smtp" ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "15.04",
        "15.10",
        "16.04",
        "16.10"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "Solaris",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.2.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.2.0 < 3.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.7.0 < 5.0.0"
    }
  ]
